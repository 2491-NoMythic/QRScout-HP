          "required": false,
          "code": "DefenseTypeTele",
          "formResetBehavior": "reset",
          "choices": {
            "Disruption": "Disrupting Shots",
            "Fuel stealing": "Fuel Stealing",
            "Blocking Trenches": "Blocking Trenches",
            "Blocking Bumps": "Blocking Bumps",
            "Blocking Shots": "Blocking Shots"
